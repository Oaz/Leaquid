<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:viewModels="clr-namespace:Leaquid.UserInterface.ViewModels"
        xmlns:views="clr-namespace:Leaquid.UserInterface.Views"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        Width="800" Height="450"
        x:Class="Leaquid.UserInterface.Views.MainWindow"
        x:DataType="viewModels:MainViewModel"
        Icon="/Assets/avalonia-logo.ico"
        Title="Leaquid">
    
        <Window.KeyBindings>
                <KeyBinding 
                        Gesture="Escape" 
                        Command="{Binding BackToHome, Mode=OneWay}" />
        </Window.KeyBindings>
        
        <views:MainView />
</Window>
